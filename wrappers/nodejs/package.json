{
  "name": "indy-sdk",
  "version": "0.0.0",
  "description": "Native bindings for hyperledger indy",
  "author": "hyperledger",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger/indy-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hyperledger/indy-sdk/issues"
  },
  "homepage": "https://github.com/hyperledger/indy-sdk/tree/master/wrappers/nodejs#readme",
  "main": "src/index.js",
  "gypfile": true,
  "engines": {
    "node": ">=8"
  },
  "files": [
    "src",
    "binding.gyp",
    "include",
    "codegen"
  ],
  "scripts": {
    "prepublish": "cp -r ../../libindy/include/ .",
    "preinstall": "node codegen",
    "install": "node-gyp rebuild",
    "test": "standard && ava --fail-fast tests",
    "fmt": "standard --fix",
    "rebuild": "node codegen && node-gyp rebuild"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "nan": "^2.8.0",
    "node-gyp": "^3.6.2"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "cuid": "^2.1.0",
    "json-stringify-pretty-compact": "^1.1.0",
    "standard": "*",
    "tmp": "0.0.33"
  }
}
