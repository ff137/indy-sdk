{
  "name": "indy-sdk",
  "version": "0.0.0",
  "description": "Native bindings for hyperledger indy",
  "author": "hyperledger",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hyperledger/indy-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/hyperledger/indy-sdk/issues"
  },
  "homepage": "https://github.com/hyperledger/indy-sdk/tree/master/wrappers/nodejs#readme",
  "main": "src/index.js",
  "gypfile": true,
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "test": "standard && RUST_LOG=trace TEST_POOL_IP=10.0.0.2 ava --fail-fast tests",
    "fmt": "standard --fix",
    "dev": "scriptsp --raw dev:watch-src dev:watch-tests",
    "dev:watch-src": "onchange -i src/ -- npm run dev:build-n-test -s",
    "dev:watch-tests": "RUST_LOG=trace TEST_POOL_IP=10.0.0.2 ava --fail-fast tests --watch",
    "dev:build-n-test": "node codegen && node-gyp rebuild && touch tests/index.js"
  },
  "dependencies": {
    "bindings": "^1.3.0",
    "nan": "^2.8.0",
    "node-gyp": "^3.6.2",
    "tmp": "0.0.33"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "onchange": "^3.3.0",
    "scriptsp": "^1.0.1",
    "standard": "*"
  }
}
